    "require": {
        "hiqdev/hiam-core": "*@dev"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0 || dev-master",
        "hiqdev/hidev-hiqdev": "<2.0 || dev-master"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\hiam\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "defines": "src/config/defines.php",
            "params": [
                "src/config/params.php",
                "?src/config/params-local.php"
            ],
            "hisite": "src/config/hisite.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
